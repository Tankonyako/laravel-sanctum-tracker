{
    "name": "alajusticia/laravel-sanctum-tracker",
    "description": "Track Sanctum tokens in Laravel.",
    "license": "MIT",
    "authors": [
        {
            "name": "Anthony Lajusticia",
            "email": "anthonylajusticia@gmail.com",
            "homepage": "https://anthonylajusticia.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "alajusticia/laravel-expirable": "^1.5",
        "guzzlehttp/guzzle": "^7.3",
        "illuminate/database": "^7.0|^8.0",
        "illuminate/queue": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "laravel/sanctum": "^2.9",
        "nesbot/carbon": "^2.53"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.0",
        "whichbrowser/parser": "^2.1",
        "jenssegers/agent": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "ALajusticia\\SanctumTracker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ALajusticia\\SanctumTracker\\Tests\\": "tests",
            "ALajusticia\\SanctumTracker\\Tests\\Database\\Factories\\": "tests/database/factories"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ALajusticia\\SanctumTracker\\SanctumTrackerServiceProvider"
            ]
        }
    }
}
